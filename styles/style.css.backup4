/* Hero Register Slide-down Panel */
.hero-register-panel {
  overflow: hidden;
  max-height: 0;
  transition: max-height 0.4s cubic-bezier(0.4,0,0.2,1);
  background: #fff;
  border-radius: 8px;
  box-shadow: 0 4px 16px rgba(0,0,0,0.08);
  margin-top: 12px;
  padding: 12px 14px 8px 14px;
  z-index: auto;
  position: static; /* participate in normal flow so overlay grows */
  right: auto;
  width: 50%;
  max-width: 560px;
  min-width: 0;
  margin-left: auto; /* right align within overlay */
  display: block; /* remain in DOM for height transition */
  visibility: hidden; /* hidden by default */
}
.hero-register-panel.open {
  visibility: visible; /* show when open */
  max-height: 600px;
  transition: max-height 0.4s cubic-bezier(0.4,0,0.2,1);
}
.register-panel-section {
  margin-bottom: 12px;
