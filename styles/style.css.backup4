  height: 70px;
  filter: drop-shadow(0 2px 8px rgba(164, 51, 43, 0.3));
}

.luxury-brand:hover .brand-logo-prominent {
  transform: scale(1.05);
  filter: drop-shadow(0 6px 16px rgba(164, 51, 43, 0.4));
