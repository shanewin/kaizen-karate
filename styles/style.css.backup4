  transform: translateX(-50%) translateY(-10px);
  transition: all 0.3s cubic-bezier(0.23, 1, 0.32, 1);
  z-index: 1000;
}

.mega-dropdown:hover .mega-dropdown-menu {
  opacity: 1;
  visibility: visible;
  transform: translateX(-50%) translateY(0);
}

