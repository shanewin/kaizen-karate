  color: #ffffff;
  font-weight: 700;
  text-transform: uppercase;
  letter-spacing: 0.6px;
  font-size: 0.85rem;
}

/* hover lift */
.served-state:hover {
  transform: translateY(-3px);
  filter: brightness(1.05);
}

