  position: relative;
  overflow: hidden;
  text-decoration: none;
  display: inline-block;
}

.btn-inverse-primary::before {
  content: '';
  position: absolute;
  top: 0;
  left: -100%;
  width: 100%;
  height: 100%;
  background: linear-gradient(90deg, transparent, rgba(255, 255, 255, 0.3), transparent);
